<div class="body-padding">
<%= content_for(:page_title, " | categories") %>
<% if @categories.any? %>

<div class="quizzle-card-container mdl-grid">
		<% @categories.each do |category| %>
				<% cache category do %>
	  			<%= render category %>
				<% end %>
		<% end %>
</div>

<div id="infinite-scrolling">
	<%= will_paginate @categories %>
</div>


<% else %>

 <%= render 'layouts/no_data' %>

<% end %>
</div>
