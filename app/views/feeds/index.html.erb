<%= content_for :page_title do %>

 | <%= (@category.category_name).downcase %>

<% end %>

<% if @feeds.any? %>

<br>
<div class="blog container">

		<div class="row">
				<div id="pins" class="blog-list">
					<% @feeds.each do |feed| %>
						<% cache feed do %>
							<%= render feed %>
						<% end %>
					<% end %>
        </div>
    </div>


<div id="infinite-scrolling">
	<%= will_paginate @feeds %>
</div>
</div>

<br><br><br><br><br><br>

<% else %>

 <%= render 'layouts/no_data' %>

<% end %>
