<br><br>
<div class="blog container">

		<div class="row">
				<div id="blog-mansonry" class="blog-list">
			  <% @feeds.each do |feed| %>
	<article class="post col-md-4 col-sm-6 col-xs-12">
								<div class="post-inner">
										<figure class="post-thumb">
											<div class="item">
												<%= image_tag("munch-eat.jpg", :size => "400x300", :class => "img-responsive") %>
											</div>
									</figure>
							<div class="content">
									<h3 class="post-title"><%= link_to feed.feed_name, feed_feed_entries_path(feed.id) %></h3>
										<div class="post-entry">
											<p>so stuff goes here hey</p>

									</div>
									<div class="meta">
											<ul class="meta-list list-inline">
												<li class="post-time post_date date updated">created <%= time_ago_in_words(feed.created_at) %> ago</li>
										</ul><!--//meta-list-->
									</div><!--meta-->
							</div><!--//content-->
					</div><!--//post-inner-->
					</article>

  <% end %>
</div>
</div>
</div>
