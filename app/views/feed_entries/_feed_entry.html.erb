<li>
   <div class="timeline-badge"><%= image_tag feed_entry.feed.category.category_image_url, :class => "roundedImage" rescue nil %></div>

   <div class="timeline-panel">
   <div class="row">
   <div class="col-md-3">
     <% if feed_entry.feed_entry_images.any? %>
        <% if feed_entry.feed_entry_images.first.img_url =~ /(twitter)|(hsdu).*/i %>
          <%= image_tag("newspaper.png", :size => "400x300", :class => "img-responsive") %>
        <% else %>
          <%= image_tag(feed_entry.feed_entry_images.first.img_url, :class => "img-responsive") rescue nil %>
        <% end %>
     <% else %>
       <%= image_tag("newspaper.png", :class => "img-responsive") %>
     <% end %>
   </div>

<div class="col-md-9">
           <div class="timeline-heading">
             <h4 class="timeline-title"><%= link_to feed_entry.title, category_feed_feed_entry_path(feed_entry.feed.category.id, feed_entry.feed.friendly_id, feed_entry.id) %></h4>
             <p><%= feed_entry.score %> score | <%= feed_entry.impressions.count %> views | published <%= distance_of_time_in_words(Time.now, feed_entry.published) %> ago in <%= link_to feed_entry.feed.feed_name, category_feed_feed_entries_path(feed_entry.feed.category_id, feed_entry.feed.id) %> by <%= feed_entry.author %></p>
           </div>
           <div class="timeline-body">
             <p>
               <% if feed_entry.summary.blank? %>
                 <%= strip_tags(feed_entry.feed_entry_content).truncate(300) %>
               <% else %>
                 <%= strip_tags(feed_entry.summary).truncate(300) %>
               <% end %>
             </p>
           </div>
         </div>
    </div>
         </div>
</li>
