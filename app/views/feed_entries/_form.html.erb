<%= simple_form_for(@feed_entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :feed %>
    <%= f.input :title %>
    <%= f.input :feed_entry_url %>
    <%= f.input :published %>
    <%= f.input :author %>
    <%= f.input :feed_entry_content %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
