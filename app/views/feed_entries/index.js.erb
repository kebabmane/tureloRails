$boxes = $('<%= j render(@feed_entries) %>')

$('#entries').append( $boxes ).imagesLoaded( function(){
  $('#entries').masonry( 'reload' );

<% if @feed_entries.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate(@feed_entries) %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>

$('#entries').masonry( 'appended', $boxes, true)

});
