<%= simple_form_for(@category, :url => turelo_admin_categories_path) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :category_name %>
    <%= f.input :category_description %>
  </div>

  <div class="field">
    <%= f.label :category_image_url %><br>
    <%= f.file_field :category_image_url %>
  </div>

<br><br><br>
  <div class="form-actions">
    <%= f.button :submit, :class=>"btn btn-primary" %>
  </div>
<% end %>
