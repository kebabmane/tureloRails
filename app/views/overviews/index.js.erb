$boxes = $('<%= j render(@feed_entries) %>')

$('#pins').append( $boxes ).imagesLoaded( function(){

<% if @feed_entries.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate(@feed_entries) %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>

$('#pins').masonry( 'appended', $boxes, true)

});
